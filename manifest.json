{
    "name": "AutoDropBot",
    "version": "0.1",
    "manifest_version": 2,
    "default_locale": "en",
    "minimum_chrome_version": "37.0.0.0",
    "description": "Supreme Bot",
    "permissions": [
        "webview",
        "power",
        "storage",
        "videoCapture",
        "geolocation",
        "pointerLock",
        "system.display",
        {
            "fileSystem": [
                "write",
                "retainEntries",
                "directory"
            ]
        },
        "accessibilityFeatures.read",
        "accessibilityFeatures.modify"
    ],
    "app": {
        "urls": ["autodropbot.github.io"],
        "background": {
            "scripts": [
                "js/foam.js",
                "js/cab.js",
                "config.js",
                "background_main.js"
            ]
        },
        "kiosk_enabled": true,
        "default_locale": "en",
        "icons": {
            "128": "128.png"
        }
    }
}